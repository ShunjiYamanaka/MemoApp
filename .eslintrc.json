{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": 0,
    "quotes": 0,
    "import/no-unresolved": "off",
    "space-before-function-paren": "off",
    "func-names": "off",
    "object-curly-newline": "off"
  }
}
